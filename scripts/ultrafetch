        "$(display_row "ğŸš€" "Public IP" "${C_MAGENTA}$(get_public_ip)${C_RESET}")"
        "${C_BOLD}${C_CYAN}â””â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”˜${C_RESET}"
    )
    
    # Print side by side with better alignment
    local max_lines=${#info_lines[@]}
    [[ ${#logo_lines[@]} -gt $max_lines ]] && max_lines=${#logo_lines[@]}
    
    for ((i=0; i<max_lines; i++)); do
        printf "%-50s %b\n" "${logo_lines[i]:-}" "${info_lines[i]:-}"
    done
    echo ""
}

