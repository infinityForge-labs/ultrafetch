    echo -e "${C_BOLD}${C_CYAN}â•‘${C_RESET}              ${C_BOLD}${C_RED}âš¡ UltraFetch v${FETCH_VERSION} âš¡${C_RESET}                   ${C_BOLD}${C_CYAN}â•‘${C_RESET}"
    echo -e "${C_BOLD}${C_CYAN}â•‘${C_RESET}     ${C_DIM}Ultimate Professional System Information Tool${C_RESET}     ${C_BOLD}${C_CYAN}â•‘${C_RESET}"
    echo -e "${C_BOLD}${C_CYAN}â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•${C_RESET}\n"
}

display_with_logo() {
    clear
    display_header
    
    local logo
    logo=$(detect_and_get_logo)
    
    mapfile -t logo_lines <<< "$logo"
    
    local user_host="${C_GREEN}$(get_username)${C_RESET}${C_DIM}@${C_RESET}${C_BLUE}$(get_hostname)${C_RESET}"
    local shell_ver=$(get_shell_version)
    local shell_display="$(get_shell)"
    [[ -n "$shell_ver" ]] && shell_display="$shell_display $shell_ver"
    
    # Create a more professional layout with better spacing and alignment
    local -a info_lines=(
        ""
        "${C_BOLD}${C_CYAN}â”Œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ ${C_RED}â— SYSTEM INFO${C_CYAN} â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”${C_RESET}"
        "$(display_row "ğŸ‘¤" "User@Host" "$user_host")"
        "$(display_row "ğŸ§" "Distribution" "${C_GREEN}$(get_distro)${C_RESET}")"
        "$(display_row "ğŸ§ " "Kernel" "${C_BLUE}$(get_kernel)${C_RESET}")"
        "$(display_row "ğŸ—ï¸ " "Architecture" "${C_PURPLE}$(get_architecture)${C_RESET}")"
        "$(display_row "â±ï¸ " "Uptime" "${C_MAGENTA}$(get_uptime)${C_RESET}")"
        "$(display_row "ğŸš" "Shell" "${C_PURPLE}$shell_display${C_RESET}")"
        "$(display_row "ğŸ“¦" "Packages" "${C_ORANGE}$(get_packages)${C_RESET}")"
        "$(display_row "ğŸ’»" "Terminal" "${C_TEAL}$(get_terminal)${C_RESET}")"
    )
    
    # Add DE/WM if available
    local de_wm=$(get_de_wm)
    if [[ -n "$de_wm" ]]; then
        info_lines+=("$(display_row "ğŸ–¥ï¸ " "DE/WM" "${C_CYAN}$de_wm${C_RESET}")")
    fi
    
    info_lines+=(
        "${C_BOLD}${C_CYAN}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ ${C_RED}â— HARDWARE${C_CYAN} â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤${C_RESET}"
        "$(display_row "âš™ï¸ " "CPU" "${C_LIME}$(get_cpu_info)${C_RESET}")"
        "$(display_row "ğŸ®" "GPU" "${C_TEAL}$(get_gpu_info)${C_RESET}")"
    )
    
    read -r mem_used mem_total mem_pct <<< "$(get_memory_info)"
    info_lines+=("$(display_row "ğŸ’¾" "Memory" "${C_GREEN}$mem_used ${C_DIM}/${C_RESET} ${C_GREEN}$mem_total ${C_DIM}(${mem_pct}%)${C_RESET}")")
    info_lines+=("${C_DIM}                       â””â”€ $(draw_bar "$mem_pct")${C_RESET}")
    
    read -r swap_used swap_total swap_pct <<< "$(get_swap_info)"
    if [[ "$swap_total" != "0 B" ]]; then
        info_lines+=("$(display_row "ğŸ’¿" "Swap" "${C_CYAN}$swap_used ${C_DIM}/${C_RESET} ${C_CYAN}$swap_total ${C_DIM}(${swap_pct}%)${C_RESET}")")
        info_lines+=("${C_DIM}                       â””â”€ $(draw_bar "$swap_pct")${C_RESET}")
    fi
    
    info_lines+=(
        "${C_BOLD}${C_CYAN}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ ${C_RED}â— PERFORMANCE${C_CYAN} â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤${C_RESET}"
        "$(display_row "ğŸ“ˆ" "Load Average" "${C_YELLOW}$(get_load_average)${C_RESET}")"
        "$(display_row "ğŸŒ¡ï¸ " "CPU Temp" "${C_ORANGE}$(get_cpu_temperature)${C_RESET}")"
        "${C_BOLD}${C_CYAN}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ ${C_RED}â— STORAGE${C_CYAN} â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤${C_RESET}"
        "$(display_row "ğŸ—„ï¸ " "Root (/)" "${C_BLUE}$(get_disk_stats "/")${C_RESET}")"
    )
    
    # Add optional mounts
    if mountpoint -q /var 2>/dev/null; then
        info_lines+=("$(display_row "ğŸ“" "/var" "${C_PURPLE}$(get_disk_stats "/var")${C_RESET}")")
    fi
    
    if mountpoint -q /home 2>/dev/null; then
        info_lines+=("$(display_row "ğŸ " "/home" "${C_PINK}$(get_disk_stats "/home")${C_RESET}")")
    fi
    
    info_lines+=(
        "${C_BOLD}${C_CYAN}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ ${C_RED}â— NETWORK${C_CYAN} â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤${C_RESET}"
